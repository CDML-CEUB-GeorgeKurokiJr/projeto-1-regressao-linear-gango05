      "cell_type": "code",
      "source": [
        "# 1. Condition: Quantity is 0 or negative\n",
        "negative_qty = retail_df['Quantity'] <= 0\n",
        "\n",
        "# 2. Condition: InvoiceNo does NOT start with 'c' or 'C'\n",
        "no_c_in_invoice = ~retail_df['InvoiceNo'].astype(str).str.lower().str.startswith('c')\n",
        "\n",
        "# 3. NEW Condition: The Description column is NOT empty (NaN)\n",
        "has_description = retail_df['Description'].notna()\n",
        "\n",
        "# 4. Combine all three conditions to isolate our specific \"ghost\" rows\n",
        "ghost_rows_with_desc = retail_df[negative_qty & no_c_in_invoice & has_description]\n",
        "\n",
        "print(f\"Total 'ghost' rows with descriptions: {len(ghost_rows_with_desc)}\\n\")\n",
        "\n",
        "# 5. Look at the specific columns\n",
        "print(\"--- FIRST 10 GHOST ROWS (WITH DESCRIPTIONS) ---\")\n",
        "display(ghost_rows_with_desc[['InvoiceNo', 'StockCode', 'Description', 'Quantity', 'UnitPrice', 'CustomerID']].head(10))\n",
        "\n",
        "# 6. Look at the most common descriptions for these weird rows!\n",
        "print(\"\\n--- MOST COMMON DESCRIPTIONS FOR THESE ROWS ---\")\n",
        "display(ghost_rows_with_desc['Description'].value_counts().head(20))"
      ],
      "metadata": {
        "id": "JkdWSshqMlso"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# 1. Glue the IDs and Features together\n",
        "retail_df = pd.concat([online_retail.data.ids, online_retail.data.features], axis=1)\n",
        "\n",
        "# 2. Isolate Real Customers\n",
        "df_clean = retail_df.dropna(subset=['CustomerID']).copy()\n",
        "\n",
        "# Identify the true cancellations first\n",
        "is_cancelled_initial = df_clean['InvoiceNo'].astype(str).str.lower().str.startswith('c')\n",
        "\n",
        "# Keep ONLY normal purchases (Quantity > 0) OR true cancellations\n",
        "valid_rows_mask = (df_clean['Quantity'] > 0) | is_cancelled_initial\n",
        "df_clean = df_clean[valid_rows_mask].copy()\n",
        "\n",
        "# 3. Build the Target Variable: Total Transaction Value\n",
        "df_clean['Transaction_Value'] = df_clean['Quantity'] * df_clean['UnitPrice']\n",
        "\n",
        "# Re-create the cancellation mask for our newly cleaned dataframe\n",
        "is_cancelled = df_clean['InvoiceNo'].astype(str).str.lower().str.startswith('c')\n",
        "\n",
        "# 4. Separate Purchases from Cancellations\n",
        "# If it is a true cancellation, log the absolute quantity.\n",
        "df_clean['Cancelled_Qty'] = np.where(is_cancelled, abs(df_clean['Quantity']), 0)\n",
        "\n",
        "df_clean['Purchased_Qty'] = np.where(~is_cancelled, df_clean['Quantity'], 0)\n",
        "\n",
        "# 5. Group by Customer to build our Machine Learning Features\n",
        "customer_profiles = df_clean.groupby('CustomerID').agg(\n",
        "    Total_Spend=('Transaction_Value', 'sum'),\n",
        "    Total_Items_Bought=('Purchased_Qty', 'sum'),\n",
        "    Total_Items_Cancelled=('Cancelled_Qty', 'sum'),\n",
        "    Total_Transactions=('InvoiceNo', 'nunique')\n",
        ").reset_index()\n",
        "\n",
        "# Look at our perfectly clean, model-ready dataset!\n",
        "display(customer_profiles.head())"
      ],
